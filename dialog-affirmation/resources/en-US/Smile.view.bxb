result-view {
  match {
    Smile (smile) {
      max (One)
    }
  }

  message {
    macro (SMILE_RESULT)
  }

  render {
    layout {
      section {
        content {
          single-line {
            text {
              value ("#{raw(smile)}")
              style (Title_XXL)
            }
          }
        }
      }
    }
  }
}